<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SergioDelgadoProyecto.formObjetivos.ventanaObjetivos">
    <ContentPage.Content>

        <StackLayout HorizontalOptions="Center">
            <Label x:Name="ide" Text="{Binding Path=id}"/>
            <Label x:Name="objetives" Text="{Binding Path=objetivos}"/>
            <Label x:Name="Fecha" Text="{Binding Path=fecha}"/>
            <Label x:Name="Registro" Text="{Binding Path=registro_id}"/>


            <Button Text="Agregar" Clicked="OnSaveButtonClicked" />
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>